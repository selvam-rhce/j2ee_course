<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
      
<hibernate-mapping>  
 <class name="dev.selvam.assignment11.models.Employee" table="employee">  
  <id name="id" column="id">  
   <generator class="assigned"></generator>  
  </id>                
  <property name="name" column="name"></property>  
  <property name="basic" column="basic"></property>  
  <property name="hra" column="hra"></property>  
  <property name="da" column="da"></property>  
  <property name="deductions" column="deductions"></property>  
 </class>                
</hibernate-mapping>